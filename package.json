    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "2.2.0",
    "eslint-plugin-react": "7.2.1",
    "husky": "0.14.3",
    "lint-staged": "4.0.3",
    "prettier": "1.5.3"
  }
